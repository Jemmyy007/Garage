        <Button Content="Список гаражей" HorizontalAlignment="Left" Margin="117,0,0,0" VerticalAlignment="Center" FontSize="24" Height="63" Click="Button_Click_2" Foreground="White" Background="#FFD07777" FontWeight="Bold" Width="232"/>
        <Button Content="Оформить заявку" HorizontalAlignment="Left" Margin="451,0,0,0" VerticalAlignment="Center" FontSize="24" Height="63" Click="Button_Click" Background="#FFD07777" Foreground="White" FontWeight="Bold" Width="235" OpacityMask="#FFD07777"/>
