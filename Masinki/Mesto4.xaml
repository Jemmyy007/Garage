<Window x:Class="masinki.Mesto4"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:masinki"
        mc:Ignorable="d"
        Title="Mesto4" Height="450" Width="800">
    <Grid Background="#FF68619A">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="62*"/>
            <ColumnDefinition Width="689*"/>
            <ColumnDefinition Width="49*"/>
        </Grid.ColumnDefinitions>
        <Image HorizontalAlignment="Left" Height="375" VerticalAlignment="Center" Width="632" Source="/4.jpg" Grid.Column="1" Margin="22,0,0,0"/>
        <Rectangle HorizontalAlignment="Center" Height="34" Margin="0,20,0,0" Stroke="#FFD07777" VerticalAlignment="Top" Width="40" Fill="#FFD07777"/>
        <Button Content="" HorizontalAlignment="Center" Margin="0,20,0,0" VerticalAlignment="Top" Width="40" Height="34" BorderBrush="#FFD07777" Click="Button_Click">
            <Button.Background>
                <ImageBrush ImageSource="/icons8-назад-48.png"/>
            </Button.Background>
        </Button>

    </Grid>
</Window>
